{
    "name": "@pure-rollup/rollup",
    "version": "1.0.0",
    "description": "Core rollup implementation",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "files": [
      "dist",
      "src"
    ],
    "scripts": {
      "build": "tsc -p tsconfig.build.json",
      "dev": "tsc -p tsconfig.build.json --watch",
      "test": "vitest",
      "test:ui": "vitest --ui",
      "clean": "rm -rf dist",
      "type-check": "tsc --noEmit"
    },
    "dependencies": {
      "acorn": "^8.11.2",
      "acorn-walk": "^8.3.0",
      "magic-string": "^0.30.5",
      "estree-walker": "^3.0.3",
      "is-reference": "^3.0.2",
      "resolve": "^1.22.8"
    },
    "devDependencies": {
      "@types/acorn": "^4.0.6",
      "@types/estree": "^1.0.5",
      "@types/resolve": "^1.20.6",
      "typescript": "^5.3.0",
      "vitest": "^1.0.0"
    },
    "engines": {
      "node": ">=18.0.0"
    },
    "keywords": [
      "rollup",
      "bundler",
      "javascript",
      "typescript",
      "build-tool"
    ],
    "author": "Your Name",
    "license": "MIT"
  }